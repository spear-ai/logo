{
  "name": "@spear-ai/logo",
  "version": "1.0.2",
  "description": "Spear AI Logo",
  "license": "MIT",
  "files": ["assets", "react"],
  "exports": {
    "./assets/*": "./assets/*",
    "./react": "./react/index.js"
  },
  "repository": "github:spear-ai/logo",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": ["*"]
    }
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:code && yarn run build:types",
    "build:code": "esbuild --outfile=react/index.js --platform=node --target=node14 src/react/index.tsx",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json",
    "check": "yarn run lint && yarn run typecheck",
    "ci": "act --container-architecture=linux/amd64 --platform=ubuntu-latest=ghcr.io/catthehacker/ubuntu:js-latest",
    "clean": "del react",
    "esbuild": "esbuild",
    "lint": "yarn run lint:js && yarn run lint:package",
    "lint:js": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:package": "npmPkgJsonLint .",
    "outdated": "yarn outdated",
    "typecheck": "tsc --noEmit",
    "update": "yarn run update:js && yarn run update:yarn",
    "update:js": "yarn upgrade-interactive --latest",
    "update:yarn": "yarn policies set-version",
    "watch": "concurrently 'yarn:watch:code' 'yarn:watch:types'",
    "watch:code": "yarn run build:code --watch",
    "watch:types": "yarn run build:types --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.12.1",
    "concurrently": "7.0.0",
    "del-cli": "4.0.1",
    "esbuild": "0.14.23",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-use-props": "1.0.0",
    "eslint-plugin-sonarjs": "0.12.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unicorn": "41.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "npm-package-json-lint": "5.4.2",
    "npm-package-json-lint-config-default": "3.0.0",
    "react": "17.0.2",
    "release-please": "13.4.12",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "sideEffects": false
}
